<div class="wikigrok">
	<div class="wg-close">
		<button class="cancel icon icon-16px icon-cancel">{{closeMsg}}</button>
	</div>
	<p class="wg-header">{{headerMsg}}</p>
	<p class="wg-content">{{contentMsg}}</p>
	<p class="wg-buttons">
		{{#buttons}}
		<button class="{{classes}}">{{label}}</button>
		{{/buttons}}
	</p>
</div>
